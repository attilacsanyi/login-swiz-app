<?xml version="1.0" encoding="utf-8"?>
<swiz:BeanProvider
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:swiz="http://swiz.swizframework.org"
	xmlns:service="com.login.service.*"
	xmlns:controller="com.login.controller.*">
	
	<!-- Controllers beans -->
    <controller:SecurityController id="securityController"/>
	<controller:UserController id="userController"/>
	<controller:AuthorityController id="authorityController"/>
	
	<!-- Services beans -->
    <service:SecurityService id="securityService"/>
	<service:UserService id="userService"/>
	<service:AuthorityService id="authorityService"/>
		
</swiz:BeanProvider>
